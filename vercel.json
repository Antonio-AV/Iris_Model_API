{
    "version": 2,
    "builds": [
        {
            "src": "api_modelo.py",
            "use": "@vercel/python",
            "config": {
                "installCommand": "ls -al && pip install -r requirements.txt --no-cache-dir --verbose"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api_modelo.py"
        }
    ]
}
