{
    "version": 2,
    "builds": [
        {
            "src": "api/api_modelo.py",
            "use": "@vercel/python",
            "config": {
                "installCommand": "pip install -r requirements.txt && pip freeze"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/api_modelo.py"
        }
    ]
}
